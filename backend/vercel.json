{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/admins/(.*)", "dest": "index.js" },
    { "src": "/api/drivers/(.*)", "dest": "index.js" },
    { "src": "/api/pelanggans/(.*)", "dest": "index.js" },
    { "src": "/api/orders/(.*)", "dest": "index.js" },
    { "src": "/api/test/(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
